  },
  "contributors": [
    {
      "name": "Jonas Enge",
      "email": "jonas.enge@gmail.com",
      "url": "https://github.com/maccyber"
    }
  ],
  "keywords": [
    "bigfive",
    "personality",
    "IPIP",
    "next",
    "compare"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zrrrzzt/bigfive-compare.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/bigfive-compare/issues"
  },
  "homepage": "https://github.com/zrrrzzt/bigfive-compare",
  "engines": {
    "node": ">=8.15.0"
  },
  "now": {
    "alias": [
      "frontpage.compare.bigfive.world"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now --npm -E production.env && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "devDependencies": {
    "ava": "2.3.0",
    "coveralls": "3.0.6",
    "nyc": "14.1.1",
    "standard": "14.2.0"
  },
  "dependencies": {
    "axios": "0.19.0",
    "bson-objectid": "1.3.0",
    "cookie": "0.4.0",
    "jws": "3.2.2",
    "muicss": "0.9.43",
    "next": "9.0.6",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-google-charts": "3.0.15",
    "simple-encryptor": "3.0.0"
  }
}
